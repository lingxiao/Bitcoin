{
  "name": "websocket-node-express",
  "version": "1.0.0",
  "description": "node websocket minimal application",
  "main": "app.js",
  "scripts": {
    "clean": "rimraf dist",
    "start": "npm-run-all clean --parallel watch:build watch:server --print-label",
    "watch:build": "tsc --watch",
    "watch:server": "nodemon './dist/index.js' --watch './dist'"
  },
  "keywords": [
    "node.js",
    "express.js",
    "websocket"
  ],
  "author": "Xiao Ling",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.11.0",
    "@types/ws": "^3.2.1",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "express": "^4.16.2",
    "websocket": "^1.0.25",
    "ws": "^4.0.0"
  }
}
